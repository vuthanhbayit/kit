{
  "name": "@vt7/kit",
  "description": "",
  "private": true,
  "repository": "@vt7/kit",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "build": "pnpm --filter './packages/**' prepack",
    "build:stub": "pnpm --filter './packages/**' prepack --stub",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs --ignore-path .gitignore .",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs --ignore-path .gitignore . --fix",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "prepublishOnly": "pnpm build"
  },
  "resolutions": {
    "@vt7/fetch": "workspace:*",
    "@vt7/iconify": "workspace:*",
    "@vt7/image": "workspace:*",
    "@vt7/tailwind-purge-icon": "workspace:*",
    "@vt7/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.52.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.20.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.9.0"
}
