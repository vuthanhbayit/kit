{
  "name": "@vt7/iconify",
  "version": "5.2.1",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/vuthanhbayit/kit/tree/main/packages/iconify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuthanhbayit/kit/tree/main/packages/iconify"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "gen-icon": "./bin/index.js"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "vitest dev",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "prepublishOnly": "pnpm build",
    "playground": "jiti playground index.ts",
    "generate-icon": "pnpm  --input=playground/icons --output=playground/icon-bash.json --prefix=icon",
    "release": "changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@iconify/tools": "^3.0.5",
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.1.11",
    "@vt7/utils": "workspace:*",
    "mri": "^1.2.0",
    "svgo": "^3.0.2"
  }
}
